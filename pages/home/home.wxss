@import "../template/products/products.wxss";
.search-box{
    position: sticky;
    border-radius: 10rpx;
    background: #f7f7f7;
    height: 70rpx;
    top: 0;
}
.search-box .icons{
    position: absolute;
    left: 15rpx;
    top: 10rpx;
}
.search-box input{
    font-size: 28rpx;
    padding-left: 80rpx;
    box-sizing: border-box;
    height: 70rpx;
    line-height: 70rpx;
    color: #666;
}
.swiper-item image{
    width: 100%;
    height: 100%;

}
.swiper-item{
    width: 100%;
    height: 100%;
}
.swiper{
    width: 100%;
    height:400rpx;
}
.home-main-header{
    display: flex;
    justify-content: center;
    height: 60rpx;
    align-items: center;
    color: #ab956D;
    font-size: 32rpx;
    margin: 20rpx 0;
}